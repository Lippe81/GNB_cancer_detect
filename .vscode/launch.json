{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "cppdbg",
            "request": "launch",
            "name": "Launch GNB Predictor",
            "program": "${workspaceFolder}/gnb_predictor",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "build",
            "miDebuggerPath": "/usr/bin/gdb",
            "miDebuggerArgs": "",
            "logging": {
                "moduleLoad": false,
                "programOutput": true,
                "trace": false,
                "engineLogging": false
            },
            "debugServerPath": "",
            "debugServerArgs": "",
            "serverStarted": "",
            "filterStdout": true,
            "filterStderr": false,
            "targetArchitecture": "x64",
            "sourceFileMap": {
                "/mnt/c": "c:\\"
            },
            "pipeTransport": {
                "pipeCwd": "",
                "pipeProgram": "bash",
                "pipeArgs": ["-c"],
                "debuggerPath": "/usr/bin/gdb"
            },
            "showDisplayString": true
        }
    ]
}
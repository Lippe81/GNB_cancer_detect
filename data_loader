#include <stdio.h>
#include <stdlib.h>

void read_csv_file(const char *filename) {
    FILE *file = fopen(filename, "r");
    if (file == NULL) {
        fprintf(stderr, "Failed to open file: %s\n", filename);
        return;
    }

    char line[1024];
    while (fgets(line, sizeof(line), file)) {
        printf("%s", line);
    }

    fclose(file);
}

int main() {
    const char *csv_filename = "breast-cancer.csv";
    read_csv_file(csv_filename);
    return 0;
}
